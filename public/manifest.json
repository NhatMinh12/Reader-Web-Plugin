
{
  "manifest_version": 3,
  "name": "AI Reader",
  "description": "Your AI assistant to help with any questions you have.",
  "version": "1.0.0",
  "action": {
      "default_popup": "index.html",
      "default_title": "Open the popup"
  },
  "icons": {
      "16": "Address-Book16.png",
      "48": "Address-Book48.png",
      "128": "Address-Book128.png"
  },
  "permissions": [
      "scripting",
      "activeTab",
      "storage"
  ],
  "host_permissions": [
      "https://*/"
  ]
}
